<?xml version="1.0" encoding="UTF-8"?>
<Window size="300,240" caption="0,0,0,30" roundcorner="1,1,1,1">
	<Font id="0" name="微软雅黑" size="14" share="true" bold="true" />
	<Font id="1" name="微软雅黑" size="14" share="true" default="true" />
	<VerticalLayout bkcolor="0xffF4F8FF" bordersize="1" bordercolor="0xFFCFE7F4">	
		<Label text="点位参数" font="0" bkcolor="#FFC5D4F2" height="20" padding="1,1,1,1" align="center" />
		<HorizontalLayout height="26">
			<Label text="当前轴名：" padding="20,3,3,3" borderround="3,3" />
			<Label name="lable_CurrentAxis" width="120" padding="3,1,20,1" borderround="3,3" />
		</HorizontalLayout>
		<HorizontalLayout height="26">
			<Label text="轴ID：" padding="20,3,3,3" borderround="3,3" />
			<Label name="lable_AxisId" width="120" padding="3,1,20,1" borderround="3,3" />
		</HorizontalLayout>
		<HorizontalLayout height="26">
			<Label text="位置名：" padding="20,3,3,3" borderround="3,3" />
			<Edit name="edit_PosName" width="120" padding="3,1,20,1" bordercolor="#FF4EA0D1" bordersize="1" />
		</HorizontalLayout>
		<HorizontalLayout height="26">
			<Label text="示教位置（mm/°）：" padding="20,3,3,3" borderround="3,3" />
			<Edit name="edit_Distance" width="120" padding="3,1,20,1" bordercolor="#FF4EA0D1" bordersize="1" />
		</HorizontalLayout>
		<HorizontalLayout height="26">
			<Label text="步长（mm/°）：" padding="20,3,3,3" borderround="3,3" />
			<Edit name="edit_Spacing" width="120" padding="3,1,20,1" bordercolor="#FF4EA0D1" bordersize="1" />
		</HorizontalLayout>
		<HorizontalLayout height="26">
			<Label text="备注：" padding="20,3,3,3" borderround="3,3" />
			<Edit name="edit_PosMarkCn" width="120" padding="3,1,20,1" bordercolor="#FF4EA0D1" bordersize="1" />
		</HorizontalLayout>
		<Label text="功能设置" font="0" bkcolor="#FFC5D4F2" height="20" padding="1,1,1,1" align="center" />
		<HorizontalLayout height="35">
			<Button name="btn_AxisParasSave" text="保存更新参数" height="35" padding="30,3,30,3" borderround="3,3" normalimage="file='button_normal.png' corner='20,2,20,2'" hotimage="file='button_hover.png' corner='20,2,20,2'" pushedimage="file='button_pushed.png' corner='20,2,20,2'" />
			<Button name="btn_CloseDlg" text="退出参数设置" height="35" padding="30,3,30,3" borderround="3,3" normalimage="file='button_normal.png' corner='20,2,20,2'" hotimage="file='button_hover.png' corner='20,2,20,2'" pushedimage="file='button_pushed.png' corner='20,2,20,2'" />
		</HorizontalLayout>
	</VerticalLayout>
</Window>
