				</HorizontalLayout>	
				<Label text="OUTPUT" bkcolor="#FFC5D4F2" height="20" align="center" />
				<HorizontalLayout valign="top" padding="1,0,1,3">
					<List name="dut_output" vscrollbar="true"  hscrollbar="true"  header="hidden" multiline="true" wantreturn="true" valign="top" bkcolor="#FFFFFFFF" />
