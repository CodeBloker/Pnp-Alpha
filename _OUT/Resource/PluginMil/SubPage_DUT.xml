					<Label text="Data:" width="30" padding="3,3,3,3" borderround="3,3"/>
					<Edit name="edit_DutData" width="70" padding="3,3,3,3" bordercolor="#FF4EA0D1" bordersize="1" />
					<Label text="H" width="10" padding="3,3,3,3" borderround="3,3"/>
