<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Window sizebox="4,4,4,4" mininfo="600,400">
	<Font id="0" name="微软雅黑" size="14" share="true" default="true" />
	<Font id="1" name="微软雅黑" size="14" share="true" bold="true" />
	<VerticalLayout bkcolor="#FFF0F0F0">
		<HorizontalLayout>
			<VerticalLayout width="300" bordercolor="#FF768D9B" bordersize="1">
				<Label text="参数设置" bkcolor="#FFC5D4F2" font="1" height="20" padding="1,1,1,1" align="center" />
				<HorizontalLayout height="40" padding="3,3,3,3">
					<Button name="btn_clearList" text="清空列表显示" font="0" padding="3,3,3,3" borderround="3,3" normalimage="file='button_normal.png' corner='20,2,20,2'" hotimage="file='button_hover.png' corner='20,2,20,2'" pushedimage="file='button_pushed.png' corner='20,2,20,2'" />
				</HorizontalLayout>
				<Label text="ACL功能验证" bkcolor="#FFC5D4F2" font="1" height="20" padding="1,1,1,1" align="center" />
				<HorizontalLayout height="40" padding="3,3,3,3">
					<Button name="btn_getACLVal" text="获取ACL值" font="0" padding="3,3,3,3" borderround="3,3" normalimage="file='button_normal.png' corner='20,2,20,2'" hotimage="file='button_hover.png' corner='20,2,20,2'" pushedimage="file='button_pushed.png' corner='20,2,20,2'" />
				</HorizontalLayout>
				<Label text="Confocal sensor功能验证" bkcolor="#FFC5D4F2" font="1" height="20" padding="1,1,1,1" align="center" />
				<HorizontalLayout height="40" padding="3,3,3,3">
					<Button name="btn_ConfocalGetValue1" text="获取高度值1" font="0" padding="3,3,3,3" borderround="3,3" normalimage="file='button_normal.png' corner='20,2,20,2'" hotimage="file='button_hover.png' corner='20,2,20,2'" pushedimage="file='button_pushed.png' corner='20,2,20,2'" />
					<Button name="btn_ConfocalGetValue2" text="获取高度值2" font="0" padding="3,3,3,3" borderround="3,3" normalimage="file='button_normal.png' corner='20,2,20,2'" hotimage="file='button_hover.png' corner='20,2,20,2'" pushedimage="file='button_pushed.png' corner='20,2,20,2'" />
				</HorizontalLayout>
			</VerticalLayout>
			<VerticalLayout bordercolor="#FF768D9B" bordersize="1">
				<Label text="输出信息" bkcolor="#FFC5D4F2" font="1" height="20" padding="1,1,1,1" align="center" />
				<HorizontalLayout>
					<List name="tcp_output" vscrollbar="true" font="0" hscrollbar="true"  header="hidden" multiline="true" wantreturn="true" valign="top" bkcolor="#FFFFFFFF" />
				</HorizontalLayout>	
			</VerticalLayout>
		</HorizontalLayout>
	</VerticalLayout>
</Window>
